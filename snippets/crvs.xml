<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<remote name="gh"
        fetch="https://github.com/" />

  <!-- Gitlab remote -->
  <remote  name="gitlab"
           fetch="https://gitlab.com/" />
		
<remote name="corvus"
        fetch="https://github.com/Corvus-R/"
        revision="refs/heads/11" />

